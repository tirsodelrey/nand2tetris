/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
	Not(in=b, out=notb);
	And(a=nota, b=b, out=ando1);
	And(a=a, b=notb, out=ando2);
	Or(a=ando1, b=ando2, out=out);
}